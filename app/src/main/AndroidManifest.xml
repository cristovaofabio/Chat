<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.whatsapp">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activitiy.RegisterGroupActivity"
            android:parentActivityName=".Activitiy.GroupActivity"/>
        <activity
            android:name=".Activitiy.GroupActivity"
            android:parentActivityName=".Activitiy.WelcomeActivity" />
        <activity
            android:name=".Activitiy.ChatActivity"
            android:parentActivityName=".Activitiy.WelcomeActivity" />
        <activity
            android:name=".Activitiy.SettingsProfileActivity"
            android:parentActivityName=".Activitiy.WelcomeActivity" />
        <activity android:name=".Activitiy.WelcomeActivity" />
        <activity android:name=".Activitiy.RegisterActivity" />
        <activity android:name=".Activitiy.LogInActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>